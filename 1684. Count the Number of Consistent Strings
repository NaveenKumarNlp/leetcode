class Solution {
    public int countConsistentStrings(String allowed, String[] words) {
        int sum=words.length;
        int []store=new int[26];
        for(char ch:allowed.toCharArray()){
            store[ch-'a']+=1;
        }
        for(String s:words){
            for(char ch:s.toCharArray()){
                if(store[ch-'a']<=0){
                    sum--;
                    break;
                }
            }
        }
        return sum;
    }
}
