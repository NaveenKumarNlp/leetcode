class Solution {
    public int[] sortEvenOdd(int[] nums) {
        for(int i=0;i<nums.length;i=i+2){
            for(int j=i+2;j<nums.length;j=j+2){
                if(nums[i]>nums[j]){
                    int temp=nums[i];
                    nums[i]=nums[j];
                    nums[j]=temp;
                }
                if(i+1<nums.length && j+1<nums.length && nums[i+1]<nums[j+1]){
                    int temp=nums[i+1];
                    nums[i+1]=nums[j+1];
                    nums[j+1]=temp;
                }
            }
        }
        return nums;
    }
}

