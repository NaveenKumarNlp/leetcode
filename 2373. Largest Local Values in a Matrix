class Solution {
    public int[][] largestLocal(int[][] grid) {
        int len=grid.length;
        int[][]ans=new int[len-2][len-2];
        for(int i=0;i<len;i++){
            for(int j=0;j<len;j++){
                if(i+3<=len && j+3<=len){
                    int max=0;
                    for(int k=i;k<i+3;k++){
                        for(int l=j;l<j+3;l++){
                            if(grid[k][l]>max){
                                max=grid[k][l];
                            }
                        }
                    }
                    ans[i][j]=max;
                }
            }
        }
        return ans;
    }
}
