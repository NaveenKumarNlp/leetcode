class Solution {
    public int getLeastFrequentDigit(int n) {
        int []t=new int[10];
        while(n>0){
            t[n%10]++;
            n/=10;
        }
        int count=Integer.MAX_VALUE;
        int val=-1;
        for(int i=0;i<10;i++){
            if(t[i]!=0 && t[i]<count){
                count=t[i];
                val=i;
            }
        }
        return val;
    }
}
