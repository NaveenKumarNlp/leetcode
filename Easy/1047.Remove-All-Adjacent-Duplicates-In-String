class Solution {
    public String removeDuplicates(String s) {
        int end = -1;
        char[] ch = s.toCharArray();
        for (char c : ch) {
            if (end >= 0 && ch[end] == c) {
                end--;
            } else {
                ch[++end] = c;
            }
        }
        return String.valueOf(ch, 0, end + 1); 
    }
}
