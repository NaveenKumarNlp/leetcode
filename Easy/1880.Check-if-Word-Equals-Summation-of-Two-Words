class Solution {
    public int intValue(String s){
        int sum=0;
        for(int i=0;i<s.length();i++){
            sum+=(s.charAt(i)-'a');
            sum*=10;
        }
        return sum;
    }
    public boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
        return intValue(firstWord)+intValue(secondWord)==intValue(targetWord);
        
    }
}

