class Solution {
    public String majorityFrequencyGroup(String s) {
      int []temp=new int[26]; 
      for(int i=0;i<s.length();i++){
        char c=s.charAt(i);
        temp[c-'a']++;
      } 
      int []res=new int[s.length()];
      int max=0,ind=-1;
      for(int i=0;i<26;i++){
        if(temp[i]!=0){
            res[temp[i]-1]++;
        }
      }
      for(int i=s.length()-1;i>=0;i--){
        if(max<res[i]){
            ind=i+1;
            max=res[i];
        }
      }
      StringBuilder sb=new StringBuilder();
      for(int i=0;i<26;i++){
        if(temp[i]==ind){
            sb.append((char)((int)('a')+i));
        }
      }
      return sb.toString();
    }
}
