import java.util.Collections;
import java.util.Arrays;
class Solution {
    public int[] maxKDistinct(int[] nums, int k) {
        Arrays.sort(nums);
        int count=1;
        int temp=nums[0];
        for(int i:nums){
            if(i!=temp){
                count++;
                temp=i;
            }
        }
        if(count>k){
            count=k;
        }
        int []arr=new int[count];
        temp=-1;
        int index=0;
        for(int i=nums.length-1;i>=0;i--){
            int val=nums[i];
            if(val!=temp && count>0){
                count--;
                arr[index++]=temp=val;
            }
        }
        return arr;
    }
}
