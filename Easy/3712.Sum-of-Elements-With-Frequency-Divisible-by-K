class Solution {
    public int sumDivisibleByK(int[] nums, int k) {
        int max=0,sum=0;
        for(int i=0;i<nums.length;i++)
            {
                max=Math.max(nums[i],max);
            }
        int cnt[]=new int[max+1];
        for(int i=0;i<nums.length;i++)
            {
                cnt[nums[i]]++;
            }
        for(int i=1;i<cnt.length;i++)
            {
                if(cnt[i]!=0 && cnt[i]%k==0)
                sum+=i*cnt[i];
            }
        return sum;
    }
}
