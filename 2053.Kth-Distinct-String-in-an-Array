class Solution {
    public String kthDistinct(String[] arr, int k) {
        LinkedHashMap<String,Integer> map=new LinkedHashMap<>();
        int x=0;
        for(String s:arr){
            map.put(s,map.getOrDefault(s,0)+1);
        }
        for(Map.Entry<String,Integer> entry:map.entrySet()){
            if(entry.getValue()==1){
                ++x;
                if(x==k){
                    return entry.getKey();
                }
            }
        }
        return "";
    }
}
