class Solution {
    public long findTheArrayConcVal(int[] nums) {
        int i=0,j=nums.length-1;
        long sum=0;
        while(i<j){
            sum+=Long.parseLong(String.valueOf(nums[i])+String.valueOf(nums[j]));
            i++;
            j--;
        }
        if(nums.length%2!=0){
            sum+=nums[nums.length/2];
        }
        return sum;
    }
}
