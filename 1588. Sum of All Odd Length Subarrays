class Solution {
    public int sumOddLengthSubarrays(int[] arr) {
        int sum=0;
        for(int end=1;end<=arr.length;end=end+2){
            for(int i=0;i<arr.length;i++){
                for(int j=i; j<end+i && end+i<=arr.length;j++){
                    sum+=arr[j];
                }
            }
        }
        return sum;
    }
}


