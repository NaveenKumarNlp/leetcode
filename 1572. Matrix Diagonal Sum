class Solution {
    public int diagonalSum(int[][] mat) {
        int sum=0;
        int len=mat.length;
        for(int i=0;i<len;i++){
            System.out.println(mat[i][i]+" "+mat[len-i-1][i]);
            sum+=mat[i][i]+mat[len-i-1][i];
        }
        if(len%2==1){
            sum-=mat[len/2][len/2];
        }
        return sum;
    }
}



00   30
11   21
22   12
33   03

