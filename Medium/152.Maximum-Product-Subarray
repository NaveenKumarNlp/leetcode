class Solution {
    public int maxProduct(int[] nums) {
        int res=nums[0],min=nums[0],max=nums[0];
        for(int i=1;i<nums.length;i++){
            int num=nums[i];
            if(num<0){
                int temp=min;
                min=max;
                max=temp;
            }
            max=Math.max(num,max*num);
            min=Math.min(num,min*num);
            res=Math.max(max,res);
        }
        return res;
    }
}
