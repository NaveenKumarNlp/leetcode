class Solution {
    public int[] searchRange(int[] nums, int target) {
        int low = 0, high = nums.length - 1;
        int min=-1,max=-1;
        while (low <= high) {
            int mid = (low + high) / 2;
            if (nums[mid] == target) {
                min = mid;     
                high = mid-1;
            } else if (nums[mid]>target){
                high=mid-1;
            }else{
                low=mid+1;
            }
        }
        low=0;
        high=nums.length-1;
        while (low <= high) {
            int mid = (low + high) / 2;
            if (nums[mid] == target) {
                max = mid; 
            } else if (nums[mid]>target){
                high=mid-1;
            }else{
                low=mid+1;
            }
        }

        return new int[]{min,max};
    }
}
