class Solution {
    public int[] arrayRankTransform(int[] arr) {
        TreeSet<Integer> set=new TreeSet<>();
        for(int a:arr){
            set.add(a);
        }
        Iterator<Integer> value = set.iterator(); 
        HashMap<Integer,Integer> map =new HashMap<>();
        int rank=1;
        while (value.hasNext()) { 
            map.put((int)(value.next()),rank++);
        }
        for(int i=0;i<arr.length;i++){
            arr[i]=map.get(arr[i]);
        }
        return arr;
    }
}
